[mysqld]
skip-host-cache
skip-name-resolve
user = mysql
datadir = /var/lib/mysql
back_log = 50
max_connections = 800
table_open_cache = 1024
max_allowed_packet = 1024M
binlog_cache_size = 8M
max_heap_table_size = 64M
sort_buffer_size = 8M
join_buffer_size = 8M
query_cache_size = 0
query_cache_limit = 2
!includedir /etc/mysql/conf.d/
